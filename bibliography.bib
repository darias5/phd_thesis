  author = {Batterham, Philip J. and Brown, Kimberly and Trias, Angelica and Poyser, Carmel and Kazan, Dominique and Calear, Alison L.},
  date = {2022},
  journaltitle = {Australian Journal of Rural Health},
  volume = {30},
