@article{bertramCostEffectivenessThresholds2016,
  title = {Cost\textendash Effectiveness Thresholds: Pros and Cons},
  shorttitle = {Cost\textendash Effectiveness Thresholds},
  author = {Bertram, Melanie Y and Lauer, Jeremy A and De Joncheere, Kees and Edejer, Tessa and Hutubessy, Raymond and Kieny, Marie-Paule and Hill, Suzanne R},
  year = {2016},
  month = dec,
  journal = {Bulletin of the World Health Organization},
  volume = {94},
  number = {12},
  pages = {925--930},
  issn = {0042-9686},
  doi = {10.2471/BLT.15.164418},
  abstract = {Cost\textendash effectiveness analysis is used to compare the costs and outcomes of alternative policy options. Each resulting cost\textendash effectiveness ratio represents the magnitude of additional health gained per additional unit of resources spent. Cost\textendash effectiveness thresholds allow cost\textendash effectiveness ratios that represent good or very good value for money to be identified. In 2001, the World Health Organization's Commission on Macroeconomics in Health suggested cost\textendash effectiveness thresholds based on multiples of a country's per-capita gross domestic product (GDP). In some contexts, in choosing which health interventions to fund and which not to fund, these thresholds have been used as decision rules. However, experience with the use of such GDP-based thresholds in decision-making processes at country level shows them to lack country specificity and this \textendash{} in addition to uncertainty in the modelled cost\textendash effectiveness ratios \textendash{} can lead to the wrong decision on how to spend health-care resources. Cost\textendash effectiveness information should be used alongside other considerations \textendash{} e.g. budget impact and feasibility considerations \textendash{} in a transparent decision-making process, rather than in isolation based on a single threshold value. Although cost\textendash effectiveness ratios are undoubtedly informative in assessing value for money, countries should be encouraged to develop a context-specific process for decision-making that is supported by legislation, has stakeholder buy-in, for example the involvement of civil society organizations and patient groups, and is transparent, consistent and fair.},
  pmcid = {PMC5153921},
  pmid = {27994285},
  file = {C\:\\Users\\danie\\Zotero\\storage\\PNNGESMG\\Bertram et al. - 2016 - Cost–effectiveness thresholds pros and cons.pdf}
}

