@book{guillemotMadagascarClimateChange2018,
  title = {Madagascar - {{Climate}} Change and Health Diagnostic : Risks and Opportunities for Climate-Smarthealth and Nutrition Investment},
  shorttitle = {Madagascar - {{Climate}} Change and Health Diagnostic},
