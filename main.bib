@article{ozdinLevelsPredictorsAnxiety2020,
  title = {Levels and Predictors of Anxiety, Depression and Health Anxiety during {{COVID-19}} Pandemic in {{Turkish}} Society: {{The}} Importance of Gender},
  shorttitle = {Levels and Predictors of Anxiety, Depression and Health Anxiety during {{COVID-19}} Pandemic in {{Turkish}} Society},
  author = {{\"O}zdin, Sel{\c c}uk and Bayrak {\"O}zdin, {\c S}{\"u}kriye},
