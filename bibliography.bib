@article{bertramCostEffectivenessThresholds2016,
  title = {Cost–Effectiveness Thresholds: Pros and Cons},
  shorttitle = {Cost–Effectiveness Thresholds},
  author = {Bertram, Melanie Y and Lauer, Jeremy A and De Joncheere, Kees and Edejer, Tessa and Hutubessy, Raymond and Kieny, Marie-Paule and Hill, Suzanne R},
